@using BookStore.Application.DTOs.Genre;
@model IEnumerable<GetAllGenresDto>

<div class="list-group">
    <a asp-action="Index" asp-controller="Home" asp-route-id="@null" class="list-group-item list-group-item-action">All Books</a>
    @foreach(var item in Model)
    {
        <a asp-action="Index" asp-controller="Home" asp-route-id="@item.Id" class="list-group-item list-group-item-action">@item.Name</a>
    }
     
</div>